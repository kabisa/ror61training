<%= form_with(model: link) do |form| %>
  <%= error_explanation(link) %>
  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div class="field">
    <%= form.label :url %>
    <%= form.text_field :url %>
  </div>
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
